	<!--解压完成后的文件在此文件目录下（相对路径）-->
  </UpdataSystem>


  <InitSystem>
     <Url>http://221.224.10.65/Testing/InitProcedure/InitProcedureHandler.ashx</Url>
     <!--远程数据地址-->
  </InitSystem>


  <ShoesSystem>
